{
  "name": "material-dayjs-adapter",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:example": "yarn start example-material-dayjs-adapter --ssl",
    "build": "ng build",
    "build:lib": "yarn build material-dayjs-adapter",
    "build:example:prod": "ng build example-material-dayjs-adapter --configuration production",
    "ci": "yarn lint",
    "test": "ng test",
    "lint": "ng lint",
    "stats": "yarn build:example:prod --namedChunks --statsJson && webpack-bundle-analyzer dist/example-material-dayjs-adapter/stats-es2015.json",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.1",
    "@angular/cdk": "~12.2.1",
    "@angular/common": "^12.2.1",
    "@angular/compiler": "~12.2.1",
    "@angular/core": "^12.2.1",
    "@angular/forms": "~12.2.1",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "~12.2.1",
    "@angular/platform-browser-dynamic": "~12.2.1",
    "@angular/router": "~12.2.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.1",
    "@angular/cli": "~12.2.1",
    "@angular/compiler-cli": "~12.2.1",
    "@angular/language-service": "~12.2.1",
    "@angular/material-moment-adapter": "^12.2.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "dayjs": "^1.8.17",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.24.0",
    "ng-packagr": "^12.2.0",
    "protractor": "~7.0.0",
    "semantic-release": "^17.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./dist/material-dayjs-adapter"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "./dist/material-dayjs-adapter/package.json"
          ],
          "message": "Release <%= nextRelease.version %> - <%= new Date().toLocaleDateString('en-US', {year: 'numeric', month: 'short', day: 'numeric', hour: 'numeric', minute: 'numeric' }) %> [skip ci]\n\n<%= nextRelease.notes %>"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tabuckner/material-dayjs-adapter.git"
  }
}
